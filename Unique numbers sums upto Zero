int n=9;
        int j=1,k=1;
        int[] output = new int[n];

            for(int i=0;i<(n/2);i++)
            {
                output[i]=j++;
            }
            for (int i=(n/2);i<n;i++)
            {
                output[i]=-(k++);
            }

            if(n%2!=0)
            {
                output[n-1]=0;
            }

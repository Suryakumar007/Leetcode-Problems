        int nums[]={0,1,2,3,4};
        int index[]={0,1,2,2,1};

        int n=nums.length;
        int[] outputArray= new int[n];

        ArrayList<Integer> output=new ArrayList<Integer>();

        for(int i=0;i<n;i++)
        {
            output.add(index[i],nums[i]);
        }


        for(int i=0;i<n;i++)
        {
            outputArray[i]=output.get(i);
        }

        System.out.println(output);
